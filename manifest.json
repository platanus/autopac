{
  "name": "CHANGE THIS : TODO",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Super extension",
  "homepage_url": "http://google.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": false
  },
  "options_page": "src/options/index.html",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser/page action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "cookies",
    "notifications",
    "tabs",
    "declarativeContent",
    "activeTab" 
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.google.com/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}
