{
  "name": "Autopac",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Put description here",
  "homepage_url": "http://google.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js",
      "js/jquery-3.2.1.min.js"
    ],
    "persistent": false
  },
  "options_page": "src/options/index.html",
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser/page action demo",
    "default_popup": "src/browser_action/bank_action.html"
  },
  "permissions": [
    "cookies",
    "notifications",
    "tabs",
    "declarativeContent",
    "activeTab",
    "http://*/*",
    "https://*/*" 
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*" 
      ],
      "js": [
        "src/inject/inject.js"
      ],
      "css" : [
        "src/inject/inject.css",
        "js/mdl/material.min.css"
      ]
    }
  ],
  "web_accessible_resources": ["src/inject/aux_fun.js"]
}
